        "query = \"\"\"\n",
        "select\n",
        "fc.nome as nm_funcionario,\n",
        "cg.nome as nm_cargo,\n",
        "fc.pretensao_salarial,\n",
        "lt.salario\n",
        "from tb_lotacao as lt\n",
        "inner join tb_funcionario as fc on fc.id = lt.id_funcionario\n",
        "inner join tb_cargo as cg on cg.id= lt.id_cargo\n",
        "\"\"\"\n",
        "\n",
        "ps.sqldf(query, locals())\n",
